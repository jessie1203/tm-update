kafka:
  ca_rotation:
    # Schedule for running the kafka CA rotation job in cron format.
    # Ignored if kafka-ca-rotation-pkg not deployed.
    # WARNING: Make sure not to schedule for the same time as certificate rotation in the kafka-cert-rotation-pkg, this may lead to unexpected behaviour.
    #
    #   Default: '0 0 15 1 *'
    schedule: '0 0 15 1 *'
  cert_rotation:
    # Schedule for running the kafka certificate rotation job in cron format.
    # Ignored if kafka-cert-rotation-pkg not deployed.
    #
    #   Default: '0 0 1 * *'
    schedule: '0 0 1 * *'
  client:
    # Set the authentication mechanism for Kafka brokers. If not set to "none", kafka.client.disable_ssl must be set to false.
    #
    #   Default: 'mtls'
    #
    #   Accepted values: 'mtls', 'scram-sha-256', 'scram-sha-512', 'none'
    auth_mechanism: 'none'
    # Bootstrap Broker Address(es)
    brokers: 'my-kafka.kafka-ns:9092'
    # Set to true to disable SSL for Kafka client to broker communications
    #
    #   Accepted values: 'true', 'false'
    disable_ssl: 'true'
    # Set to true to use the System CA/truststore for Kafka client to broker communications over SSL
    #
    #   Default: 'false'
    #
    #   Accepted values: 'true', 'false'
    use_system_ca: 'false'
nestlings:
  db:
    # Nestlings DB host
    host: 'prod-vault-db-vip.internal.prod.icsapi.com'
    # Nestlings DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'nestlings_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'nestlings_migrator'
    # Nestlings DB name
    #
    #   Default: 'nestlings'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'nestlings'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Nestlings DB user
    #
    #   Default: 'nestlings'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'nestlings'
observability:
  # Cluster name for observability functionality - used by monitoring components
  #
  #   Default: 'cluster.name.here'
  cluster_name: 'prod-vault-cluster'
  cluster_prometheus:
    # The number of replicas for Cluster Prometheus instances.
    # Use 2 or 3 for High Availability.
    # This value should be less than or equal to the number
    # of Availability Zones of the cluster.
    #
    #   Default: 1
    #
    #   Accepted values: 1, 2, 3
    replicas: 1
  grafana:
    # Enable anonymous access to Grafana
    #
    #   Default: 'false'
    #
    #   Accepted values: 'true', 'false'
    auth_anonymous_enabled: 'true'
    # Organisation name to use when setting up Grafana
    #
    #   Default: 'Main Org.'
    auth_anonymous_org_name: 'Main Org.'
    # Role to assign to the organisation in Grafana
    #
    #   Default: 'Viewer'
    auth_anonymous_org_role: 'Viewer'
    # Set this value to true if you want to configure CA authentication for the configured datasource
    #
    #   Default: 'false'
    #
    #   Accepted values: 'false', 'true'
    caAuth: 'false'
    # In case caAuth parameter is set to true, the value of this field will be used as the certificate used for authentication. ATTENTION: CERTIFICATE HAS TO BE IN A SINGLE LINE
    caCert: '-----BEGIN CERTIFICATE-----QKEQWKLEQWKELQWKELQWKEL131231ASMD/SAMD/SAMCZXNcMZXNCASDASD434534KASMDAMWQEQWK-----END CERTIFICATE-----'
    # Username for the default admin user for Grafana
    #
    #   Default: 'vault'
    security_admin_user: 'vault'
  ingress:
    # Subdomain to use for all observability services in ingress
    #
    #   Default: 'observability.services.endpoint.domain.here'
    domain: 'observability.internal.prod.icsapi.com'
    # Labels to apply to the ingress for observability services
    #
    #   Default: {'cert-manager.io/provider': 'instances'}
    labels:
      cert-manager.io/provider: 'instances'
    # Annotations to apply to the ingress for observability services
    #
    #   Default: {'kubernetes.io/ingress.class': 'nginx-internal', 'tmachine.io/dns.class': 'private'}
    monitoring-annotations:
      kubernetes.io/ingress.class: 'nginx'
      tmachine.io/dns.class: 'private'
      nginx.ingress.kubernetes.io/proxy-connect-timeout: '36s'
      nginx.ingress.kubernetes.io/proxy-read-timeout: '600s'
      # Put place holder update this for automation
      nginx.ingress.kubernetes.io/whitelist-source-range: 0.0.0.0/0
  # The kubernetes namespace to install observability monitoring into
  #
  #   Default: 'tm-monitoring'
  monitoring_namespace: 'tm-monitoring'
  namespaced_prometheus:
    # The number of replicas for Namespaced Prometheus instances.
    # Use 2 or 3 for High Availability.
    # This value should be less than or equal to the number
    # of Availability Zones of the cluster.
    #
    #   Default: 1
    #
    #   Accepted values: 1, 2, 3
    replicas: 1
ops:
  # Name of the bank, as displayed in the operations dashboard
  bank_name: 'Nikel'
  db:
    # Hostname for the operations dashboard database
    #
    #   Default: the value of eplatform.db.host
    host: 'prod-vault-db-vip.internal.prod.icsapi.com'
    # Ops DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'ops_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'ops_migrator'
    # Name of the operations dashboard database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'ops'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'ops'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the operations dashboard database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'ops'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'ops'
  # Default title for pages in the operations dashboard
  default_page_title: 'Nikel'
  pluggable_components:
    # Payment hub pluggable component
    #
    #   Default: False
    #
    #   Accepted values: True, False
    payments_hub: False
    # Product hub pluggable component
    #
    #   Default: False
    #
    #   Accepted values: True, False
    product_hub: False

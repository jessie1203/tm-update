istio:
  auth_policy:
    # Sets whether Istio should encrypt communication between Envoy Proxy's within the service mesh.
    # 
    #   Type: bool
    #   Accepted values: True, False
    mtls_allow_tls: True
    # Mutual TLS mode to use for istio
    # 
    #   Accepted values: 'PERMISSIVE', 'STRICT'
    mtls_mode: 'PERMISSIVE'
  # The platform provider for the cluster, for use in CNI configuration.
  # 
  #   Default: ''
  # 
  #   Accepted values: 'GCP', 'AWS', 'AKS', 'OpenShift', ''
  cloud_platform: 'GCP'
  proxy:
    # List of comma-separated CIDR ranges to exclude from the istio proxy (empty string for none or "*" for all ranges).
    # When using the Istio CNI, this should include the CIDR range required to access the secrets manager.
    # 
    #   Default: ''
    exclude_cidr: ''
    # List of comma-separated ports to exclude from the service mesh (empty string for none).
    # Communication on listed ports will not be intercepted via the proxy and will be routed directly to the container.
    # Note that this list should not include the broker ports specified in kafka.client.brokers.
    #
    #   Default: ''
    exclude_outbound_ports: ''
    # List of comma-separated CIDR ranges to include for the istio proxy (empty string for none or "*" for all ranges)
    #

      #
      #   Default: {}
      annotations:
        nginx.ingress.kubernetes.io/upstream-vhost: 'xpl-api-gateway.tm-deploy-ns.svc.cluster.local'
      #   nginx.ingress.kubernetes.io/upstream-vhost: 'xpl-api-gateway.tm-deploy-ns.svc.cluster.local'
      # The name of the Kubernetes secret the xpl Ingress uses to secure traffic. This is optional as it has a default.
      #
      #   Default: 'vault-xpl-ingress-cert'
      tls_secret_name: 'vault-xpl-ingress-cert'
  # The kubernetes namespace that this vault instance will be deployed to
  namespace: 'tm-deploy-ns'
  #    > pull_image_by_digest - IMPORTANT!! Disable this field if using the docker CLI to copy images to your private registry as
  #    > 'docker pull+tag+push' will not preserve the original digests.
  #    > When enabled, reference images by digest instead of semantic tag in kubernetes manifests. This
  #    > expects the registry used with this workload to honour the original image digests. Tools such as
  #    > 'skopeo' and 'crane' will preserve digests when copying images but the docker CLI will not.
  #    > accepted_type: bool
  pull_image_by_digest: true
kafka:
  ca_rotation:
    # Schedule for running the kafka CA rotation job in cron format.
    # Ignored if kafka-ca-rotation-pkg not deployed.
    # WARNING: Make sure not to schedule for the same time as certificate rotation in the kafka-cert-rotation-pkg, this may lead to unexpected behaviour.
    #
    #   Default: '0 0 15 1 *'
    schedule: '0 0 15 1 *'
  cert_rotation:
    # Schedule for running the kafka certificate rotation job in cron format.
    # Ignored if kafka-cert-rotation-pkg not deployed.
    #
    #   Default: '0 0 1 * *'
    schedule: '0 0 1 * *'
  client:
    # Set the authentication mechanism for Kafka brokers. If not set to "none", kafka.client.disable_ssl must be set to false.
    #
    #   Default: 'mtls'
    #
    #   Accepted values: 'mtls', 'scram-sha-256', 'scram-sha-512', 'none'
    auth_mechanism: 'none'
    # Bootstrap Broker Address(es)
    brokers: 'my-kafka-cp-kafka.kafka-ns:9092'
    # Set to true to disable SSL for Kafka client to broker communications
    #
    #   Accepted values: 'true', 'false'
    disable_ssl: 'true'
    # Set to true to use the System CA/truststore for Kafka client to broker communications over SSL
    #
    #   Default: 'false'
    #
    #   Accepted values: 'true', 'false'
    use_system_ca: 'false'
  networking:
    #    > endpoint - Endpoint to expose for loadbalancer service to give access to Kafka from outside of the
    #    > Kubernetes cluster.
    endpoint: kafka.insert.client.endpoint.here
ledger_balances:
  db:
    #    > admin_user - Admin user for the ledger_balances database (used for schema initialisation)
    #    > Warning: For Vault versions instances created <3.0.1 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=3.0.1 the value can be different if using a separate physical db for ledger_balances.
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    #    > host - Hostname for the balances database
    #    > Warning: This section should ONLY be populated if using a separate physical db for ledger_balances. The DB host should be different to all the other *.db.host sections of this file. For Vault versions instances created <3.0.1 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=3.0.1 the value can be different if using a separate physical db for ledger_balances.
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    #    > migrator_user - Ledger balances DB migrator user used for running db migrations
    #    > Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #    > Warning: For Vault versions instances created <3.0.1 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=3.0.1 the value can be different if using a separate physical db for ledger_balances.
    #    > regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'ledger_balances_migrator'
    #    > name - Name of the ledger balances database
    #    > Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #    > Warning: For Vault versions instances created <3.0.1 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=3.0.1 the value can be different if using a separate physical db for ledger_balances.
    #    > regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'ledger_balances'
    #    > port - Database port to connect to.
    #    > Warning: For Vault versions instances created <3.0.1 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=3.0.1 the value can be different if using a separate physical db for ledger_balances.
    #    > regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    #    > user - Username to access the ledger balances database
    #    > Note: Hyphen characters as part of this value are supported, but recommended to be avoided.
    #    > If you are using client certificates, set this value to be equal to the common name of the
    #    > \"vault_ledger_balances\" principal, which can be found in the DB principals artefact.
    #    > Warning: For Vault versions instances created <3.0.1 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=3.0.1 the value can be different if using a separate physical db for ledger_balances.
    #    > regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'ledger_balances'
observability:
  alertmanager:
    #    > storageclass - The Kubernetes StorageClass to be used by Alertmanager volumes.
    #    > regex: ^[0-9a-z]+[-0-9a-z]*[0-9a-z]+$
    storageclass: general-encrypted
  #    > cluster_name - Cluster name for observability functionality - used by monitoring components.
  #    > This variable will control the kubernetes_cluster metrics label,
  #    > e.g. for the Cluster dropdown in Grafana dashboards.
  #    > It is recommended to use the cluster's short name or nickname.
  #    > Note: No spaces are allowed in this value and it can not be left empty
  #    > regex: ^\S+$
  cluster_name: '{env}-vault-cluster'
  cluster_prometheus:
    cardinality:
      storage:
        #    > size - Persistent storage size allocated for Prometheus Cardinality, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 4Gi
    cloud:
      storage:
        #    > size - Persistent storage size allocated for Prometheus Cloud, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 25Gi
    istio:
      storage:
        #    > size - Persistent storage size allocated for Prometheus Istio, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 25Gi
    kubelet:
      storage:
        #    > size - Persistent storage size allocated for Prometheus Kubelet, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 25Gi
    kubernetes:
      storage:
        #    > size - Persistent storage size allocated for Prometheus Kubernetes, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 35Gi
    #    > replicas - The number of replicas for Cluster Prometheus instances.
    #    > Use 2 or 3 for High Availability.
    #    > This value should be less than or equal to the number
    #    > of Availability Zones of the cluster.
    #    > accepted_values:
    #    > - 1
    #    > - 2
    #    > - 3
    replicas: 1
    #    > retention - Retention for all cluster prometheus instances, in days or weeks.
    #    > If you increase the retention, please adjust storage size as well, otherwise
    #    > Prometheus will start removing old storage blocks to avoid running out of disk space.
    #    > regex: ^[1-9][0-9]{0,3}(d|w)$
    # retention: 2w
  grafana:
    # Enable anonymous access to Grafana
    #
    #   Default: 'false'
    #
    #   Accepted values: 'true', 'false'
    auth_anonymous_enabled: 'true'
    # Organisation name to use when setting up Grafana
    #
    #   Default: 'Main Org.'
    auth_anonymous_org_name: 'Main Org.'
    # Role to assign to the organisation in Grafana
    #
    #   Default: 'Viewer'
    auth_anonymous_org_role: 'Viewer'
    # Set this value to true if you want to configure CA authentication for the configured datasource
    #
    #   Default: 'false'
    #
    #   Accepted values: 'false', 'true'
    caAuth: 'false'
    # In case caAuth parameter is set to true, the value of this field will be used as the certificate used for authentication. ATTENTION: CERTIFICATE HAS TO BE IN A SINGLE LINE
    caCert: '-----BEGIN CERTIFICATE-----QKEQWKLEQWKELQWKELQWKEL131231ASMD/SAMD/SAMCZXNcMZXNCASDASD434534KASMDAMWQEQWK-----END CERTIFICATE-----'
    # Username for the default admin user for Grafana
    #
    #   Default: 'vault'
    security_admin_user: 'vault'
  ingress:
    # Subdomain to use for all observability services in ingress
    #
    #   Default: 'observability.services.endpoint.domain.here'
    domain: 'tm-monitoring-{env}.internal.icsapi.com'
    # Labels to apply to the ingress for observability services
    #
    #   Default: {'cert-manager.io/provider': 'instances'}
    labels:
      cert-manager.io/provider: 'instances'
    # Annotations to apply to the ingress for observability services
    #
    #   Default: {'kubernetes.io/ingress.class': 'nginx-internal', 'tmachine.io/dns.class': 'private'}
    monitoring-annotations:
      kubernetes.io/ingress.class: 'nginx-internal'
      tmachine.io/dns.class: 'private'
      nginx.ingress.kubernetes.io/proxy-connect-timeout: '36s'
      nginx.ingress.kubernetes.io/proxy-read-timeout: '600s'
      # Put place holder update this for automation
      nginx.ingress.kubernetes.io/whitelist-source-range: 0.0.0.0/0
  # The kubernetes namespace to install observability monitoring into
  #
  #   Default: 'tm-monitoring'
  monitoring_namespace: 'tm-monitoring'
  namespaced_prometheus:
    envoy:
      storage:
        #    > size - Persistent storage size allocated for Prometheus Envoy, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 30Gi
    kafka:
      storage:
        #    > size - Persistent storage size allocated for Prometheus Kafka, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 20Gi
    kubelet:
      storage:
        #    > size - Persistent storage size allocated for Namespaced Prometheus
        #    > Kubelet, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 25Gi
    kubernetes:
      storage:
        #    > size - Persistent storage size allocated for Namespaced Prometheus
        #    > Kubernetes, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 25Gi
    postgres:
      storage:
        #    > size - Persistent storage size allocated for Prometheus Postgres, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 10Gi
    #    > replicas - The number of replicas for Namespaced Prometheus instances.
    #    > Use 2 or 3 for High Availability.
    #    > This value should be less than or equal to the number
    #    > of Availability Zones of the cluster.
    #    > accepted_values:
    #    > - 1
    #    > - 2
    #    > - 3
    replicas: 1
    #    > retention - Retention period for all Namespaced Prometheus instances.
    #    > If you increase retention, please adjust storage size as well, otherwise
    #    > Prometheus will start removing old storage blocks to avoid running out of disk space.
    #    > regex: ^[1-9][0-9]{0,3}(d|w)$
    retention: 6w
    vault:
      storage:
        #    > size - Persistent storage size allocated for Prometheus Vault, in Gi.
        #    > regex: ^[1-9][0-9]{0,3}Gi$
        size: 45Gi
  node_exporter:
    #    > memory_limit - The memory request of the node exporter. Must either be between 30M and 999M, 30Mi and 999Mi, 1G and 9G or 1Gi and 9Gi. Must be expressd in M, Mi, G or Gi.
    #    > regex: ^((?:[3-9][0-9]M[i]?)|(?:[1-9][0-9][0-9]M[i]?)|(?:[1-9]G[i]?))$
    memory_limit: 256Mi
    #    > memory_request - The memory request of the node exporter. Must either be between 30M and 999M, 30Mi and 999Mi, 1G and 9G or 1Gi and 9Gi. Must be expressd in M, Mi, G or Gi.
    #    > regex: ^((?:[3-9][0-9]M[i]?)|(?:[1-9][0-9][0-9]M[i]?)|(?:[1-9]G[i]?))$
    memory_request: 32Mi
  prometheus:
    #    > storageclass - The Kubernetes StorageClass to be used by Prometheus volumes.
    #    > regex: ^[0-9a-z]+[-0-9a-z]*[0-9a-z]+$
    storageclass: prometheus
ops:
  # Name of the bank, as displayed in the operations dashboard
  bank_name: 'Nikel'
  #    > currency_display - Determines if the code and/or currency symbol should be displayed for monetary values in the Operations Dashboard, i.e. code: 1,234,567.00 GBP, symbol: £1,234,567.00, code_and_symbol: GBP £1,234,567.00
  #    > accepted_values:
  #    > - code
  #    > - symbol
  #    > - code_and_symbol
  currency_display: code
  db:
    #    > admin_user - Admin user for the database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the operations dashboard database
    #
    #   Default: the value of eplatform.db.host
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Ops DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'ops_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'ops_migrator'
    # Name of the operations dashboard database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'ops'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'ops'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the operations dashboard database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'ops'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'ops'
  # Default title for pages in the operations dashboard
  default_page_title: 'Nikel'
  #    > default_locale - This locale will be used for formatting values in the Operations Dashboard, including currencies, dates and times. It is used as fallback if no locale can be detected from the user's browser or if ops.use_browser_locale is set to false. Locale format should conform to an ISO 639-1 language code.
  default_locale: en-GB
  #    > display_timezone - Timezone used for displaying dates in the operations dashboard
  display_timezone: UTC
  pluggable_components:
    # Payment hub pluggable component
    #
    #   Default: False
    #
    #   Accepted values: True, False
    payments_hub: False
    # Product hub pluggable component
    #
    #   Default: False
    #
    #   Accepted values: True, False
    product_hub: False
    # Experience layer pluggable component
    #
    #   Default: False
    #
    #   Accepted values: True, False
    xpl: False
  #    > use_browser_locale - Determines if to use the user's browser locale when formatting values in the Operations Dashboard, including currencies, dates and times. If set to false, the value of ops.default_locale will be used.
  #    > accepted_values:
  #    > - true
  #    > - false
  use_browser_locale: true
  website:
    # The endpoint used for ingress to the operations dashboard website. This must be a subdomain of
    # common.services.domain. Cookies can only be set on the domain a request is made from. This
    # implies that all SAML protected websites must be part of the same domain.
    # (https://tools.ietf.org/html/rfc6265#section-4.1.2.3)
    endpoint: 'ops.${env}.icsapi.com'
parameter_store:
  db:
    #    > admin_user - Admin user for the Parameter Store database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    #    > host - Hostname for the Parameter Store database
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    #    > migrator_user - Parameter Store DB migrator user used for running db migrations
    #    > Note: Hyphen characters as part of this value are supported, but recommended to be avoided.
    #    > If you are using client certificates, set this value to be equal to the common name of the
    #    > \"parameter_store_migrator\" principal, which can be found in the DB principals artefact.
    #    > regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: parameter_store_migrator
    #    > name - Name of the Parameter Store database
    #    > Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #    > regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: parameter_store
    #    > port - Port number to connect to Parameter Store database.
    #    > regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    #    > user - Username to access the Parameter Store database
    #    > Note: Hyphen characters as part of this value are supported, but recommended to be avoided.
    #    > If you are using client certificates, set this value to be equal to the common name of the
    #    > \"parameter_store\" principal, which can be found in the DB principals artefact.
    #    > regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: parameter_store
payments_hub:
  # Cron expression that defines when BACS Claims should be processed. A schedule runs at the time specified
  # by the expression. If the expression is modified, the current active schedule is updated accordingly.
  # By default, the schedule is set to run at 02:00am every day relative to London timezione (UTC+0/UTC+1).
  #
  #   Default: '0 2 * * *'
  bacs_claim_schedule_expression: '0 2 * * *'
  # Cron expression that defines when BACS Credit Transfers should be processed. A schedule runs at the time specified
  # by the expression. If the expression is modified, the current active schedule is updated accordingly.
  # By default, the schedule is set to run at 12:01am every day relative to London timezione (UTC+0/UTC+1).
  #
  #   Default: '1 0 * * *'
  bacs_credit_transfer_schedule_expression: '1 0 * * *'
  # Cron expression that defines when BACS Direct Debits should be processed. A schedule runs at the time specified
  # by the expression. If the expression is modified, the current active schedule is updated accordingly.
  # By default, the schedule is set to run at 03:00am every day relative to London timezione (UTC+0/UTC+1).
  #
  #   Default: '0 3 * * *'
  bacs_direct_debit_schedule_expression: '0 3 * * *'
  # Should only be set by clients who use Bacs functionality in the Payments Hub.
  # The account for handling BACS Direct Debit returns. This account is debited to settle the initial Direct Debit and
  # later credited once the related direct debit return is processed. This account should be the same as the one defined in
  # bacs_direct_debit_unapplied_inbound_account. It has been kept for backward compatibility reasons and will be removed in the next release.
  #
  #   Default: ''
  bacs_direct_debit_unapplied_account: 'BACS_DIRECT_DEBIT_UNAPPLIED'
  # Should only be set by clients who use Bacs functionality in the Payments Hub.
  # The account for handling BACS Direct Debit returns. This account is debited to settle the initial inbound Direct Debit and
  # later credited once the related direct debit return is processed.
  #
  #   Default: ''
  bacs_direct_debit_unapplied_inbound_account: 'BACS_DIRECT_DEBIT_UNAPPLIED_INBOUND'
  # Should only be set by clients who use Bacs functionality in the Payments Hub.
  # The account for handling failed outbound Direct Debits for BACS.
  # This account is credited if crediting the Direct Debit creditor side was not possible.
  #
  #   Default: ''
  bacs_direct_debit_unapplied_outbound_account: 'BACS_DIRECT_DEBIT_UNAPPLIED_OUTBOUND'
  # A GCP bucket for storing BACS specific files, e.g. ADDACS and AUDDIS reports.
  #
  #   Default: ''
  bacs_gcp_bucket: 'tm_bacs'
  # Should only be set by clients who use Bacs functionality in the Payments Hub.
  # The internal account for BACS. Used when issuing Postings the Postings API.
  #
  #   Default: ''
  bacs_internal_account: '33'
  # Should only be set by clients who use Bacs functionality in the Payments Hub.
  # The internal account for BACS Direct Debits. Used when issuing Postings to the Postings API.
  #
  #   Default: ''
  bacs_internal_direct_debit_account: 'BACS_DIRECT_DEBIT_WASH'
  # Should only be set by clients who use Bacs functionality in the Payments Hub.
  # The internal account used for BACS Direct Debit Refunds. Used when issuing Postings to the Postings API.
  # This account is debited when a customer has been credited with a refund on an inbound Direct Debit.
  # This account is credited later on, when a refund on an inbound Direct Debit is settled with the scheme.
  #
  #   Default: ''
  #
  #   Value must satisfy regex: ^[a-z0-9A-Z\_\-]{0,36}$
  bacs_internal_direct_debit_refund_account: 'BACS_DIRECT_DEBIT_REFUND'
  # Should only be set by clients who use Bacs functionality in the Payments Hub.
  # The internal wash account used for BACS Direct Debit Refunds. Used when issuing Postings to the Postings API.
  # This account is debited when a refund of an inbound BACS Direct Debit is settled.
  #
  #   Default: ''
  #
  #   Value must satisfy regex: ^[a-z0-9A-Z\_\-]{0,36}$
  bacs_internal_direct_debit_refund_wash_account: 'BACS_DIRECT_DEBIT_REFUND_WASH'
  # Cron expression that defines when BACS Mandates should be processed. A schedule runs at the time specified
  # by the expression. If the expression is modified, the current active schedule is updated accordingly.
  # By default, the schedule is set to run at 02:00am every day relative to London timezione (UTC+0/UTC+1).
  #
  #   Default: '0 2 * * *'
  bacs_mandate_schedule_expression: '0 2 * * *'
  # Should only be set by clients who use Bacs functionality in the Payments Hub.
  # The id of the internal account to which funds are redirected in the case where pre-cleared funds cannot
  # be applied to a customer's account. Applies to BACS payments only. Only applicable if the Payments Hub is deployed.
  #
  #   Default: ''
  #
  #   Value must satisfy regex: ^[a-z0-9A-Z\_\-]{0,36}$
  bacs_unapplied_funds_account: 'BACS_CREDIT_TRANSFER_UNAPPLIED'
  # The endpoint used for ingress to the payment hub.
  endpoint: 'payments-hub-api-{env}.internal.icsapi.com'
  # An optional json object defining the exception handling behaviour; Change to {} for no
  # exception handling.
  exception_handling_cases_json: |
    {
      "INBOUND_AUTH_REJECTIONS": {
        "RESTRICTION_REQUIRES_REVIEW": {
          "REDIRECT_FUNDS": true
        }
      }
    }
  # The internal account for FPS.
  #
  #   Value must satisfy regex: ^[a-z0-9A-Z\_\-]{0,36}$
  fps_internal_account: '25'
  # A prefix that is used during the generation of a scheme_transaction_id (for FPS payments).
  fps_txnref_prefix: 'TMFPS'
  # An optional GCP bucket name that is used by the Vocalink service to store and retrieve
  # payment scheme data. Only applicable if the Payments Hub is deployed.
  #
  #   Default: ''
  gcp_bucket: 'vocalink_data_bucket'
  # A comma separated list of SWIFT BICs that are considered to be local to the Vault instance. This must be set to enable IBAN
  # creation in the Payments Hub.
  #
  #   Default: ''
  local_bics: 'BANKGB2L,BANKGB2LABC'
  # A comma separated list of sort codes that are considered to be local to the Vault instance. The Payments Hub uses
  # these to:
  # - Determine if payments should be routed as OnUs payments (rather than going out to a scheme).
  # - As a validation check during GBDSC account number generation. It is not possible to generate an account number
  #   for a sort code that is not listed here.
  local_sort_codes: '999999,999998'
  # The internal account to which funds to be redirected in exception handling cases. Applies to FPS payments only.
  #
  #   Value must satisfy regex: ^[a-z0-9A-Z\_\-]{0,36}$
  redirection_account: '33'
  # The id of the internal account to which funds are redirected in the case where pre-cleared funds cannot be applied
  # to a customer's account (for example, in the FPS execution plan "FPS_CLEARED"). Applies to FPS payments only.
  #
  #   Default: '33'
  #
  #   Value must satisfy regex: ^[a-z0-9A-Z\_\-]{0,36}$
  unapplied_funds_account: '33'
policy:
  # Time interval to control cache renewal frequency
  cacheRenewalInterval: '10s'
postings:
  api:
    # The format of public topic messages consumed or produced by the Streaming API
    #
    #   Accepted values: 'json', 'proto'
    format: 'json'
  db:
    # Root user for the postings database (used for schema initialisation)
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for postings.
    #
    #   Default: the value of vault.db.admin_user
    #
    #   Value must satisfy regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the postings database
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for postings.
    #
    #   Default: the value of vault.db.host
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Postings DB migrator user used for running db migrations
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for postings.
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: the value of vault.db.migrator_user
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'postings_migrator'
    # Name of the Postings database
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for postings.
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: the value of vault.db.name
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'postings'
    # Database port to connect to.
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for postings.
    #
    #   Default: the value of vault.db.port
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the postings database
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for postings.
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: the value of vault.db.user
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'postings'
  #    > pinned_replicas - If true, the replicas for all postings processors will be pinned to maximum. This is to optimize performance
  #    > and disregard cost which we may want to do under certain scenarios. If it is set to false, we will use the hpa
  #    > standard config that scales to max replicas quickly.
  #    > accepted_values:
  #    > - "false"
  #    > - "true"
  pinned_replicas: "false"
product_catalogue:
  db:
    #    > admin_user - Admin user for the database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the product catalogue database
    #
    #   Default: the value of vault.db.host
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Product Catalogue DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'product_catalogue_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'product_catalogue_migrator'
    # Name of the product catalogue database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'product_catalogue'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'product_catalogue'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the product catalogue database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'product_catalogue'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'product_catalogue'
products_api:
  # The endpoint used for ingress to the products API
  endpoint: 'products-api-gateway.${env}.icsapi.com'
saml_idp:
  # The SAML x509 certificate used by Vault to verify requests, provided by IDP at setup.
  # Public key in one line format string.
  #
  #   Value must satisfy regex: ^.*$
  certificate: '-----BEGIN CERTIFICATE----------END CERTIFICATE-----'
    # The key of the email SAML attribute sent by the IDP, as configured there
  email_attribute: 'urn:oid:1.2.840.113549.1.9.1'
  # The endpoint of the dummy SAML IDP, if used (ie, when saml_idp_pkg is deployed)
  endpoint: 'ssaml.${env}.dumy.icsapi.com'
  # The SAML Entity ID given by the IDP at setup
  entity_id: 'https://ops.${env}.icsapi.com/auth/realms/tm-vault/broker/saml/endpoint/descriptor'
  # Optional, the key of the name SAML attribute sent by the IDP, as configured there
  name_attribute: 'urn:oid:2.5.4.42'
  # The key of the roles SAML attribute sent by the IDP, as configured there
  roles_attribute: 'urn:oid:1.3.6.1.4.1.5923.1.1.1.1'
  # The SAML logout URL for the given IDP, usually not provided at setup but easy to find
  slo_url: 'https://saml.${env}.icsapi.com/auth/realms/tm-vault/protocol/saml'
  # The SAML SSO URL given by the IDP at setup, to which Vault will redirect
  sso_url: 'https://saml.${env}.icsapi.com/auth/realms/tm-vault/protocol/saml'
saml_sp:
  #    > authn_requests_signed - Indicates whether the SAML Service Provider will sign `<samlp:AuthnRequest>` with its private
  #    > key.
  #    > accepted_values:
  #    > - false
  #    > - true
  authn_requests_signed: false
  #    > fail_on_authn_context_mismatch - Indicates that the SAML Service Provider will only accept an authn context if it corresponds to
  #    > the requested authn context.
  #    > accepted_values:
  #    > - false
  #    > - true
  fail_on_authn_context_mismatch: false
  #    > name_id_encrypted - Specifies whether the NameID contained with `<samlp:logoutRequest>` messages sent by the SAML
  #    > Service Provider will be encrypted using the Identity Provider's certificate.
  #    > accepted_values:
  #    > - false
  #    > - true
  name_id_encrypted: false
  #    > requested_authn_context - Specifies whether the SAML Service Provider will request that the auth context be sent in the
  #    > authn request.
  #    > accepted_values:
  #    > - false
  #    > - true
  requested_authn_context: false
  #    > sign_metadata - Indicates whether the SAML Service Provider will sign its metadata using its private key.
  #    > accepted_values:
  #    > - false
  #    > - true
  sign_metadata: false
  #    > signature_algorithm - Specifies the one-way hashing algorithm used to generate a digest to be signed by the SAML
  #    > Service Provider. This digest is used in the process of generating a signed logout request.
  #    > accepted_values:
  #    > - http://www.w3.org/2001/04/xmldsig-more#rsa-sha256
  #    > - http://www.w3.org/2001/04/xmldsig-more#rsa-sha384
  #    > - http://www.w3.org/2001/04/xmldsig-more#rsa-sha512
  signature_algorithm: http://www.w3.org/2001/04/xmldsig-more#rsa-sha256
  #    > sls_req_signed - Determines whether the SAML Service Provider will sign SAML logout requests using its private
  #    > key.
  #    > accepted_values:
  #    > - false
  #    > - true
  sls_req_signed: false
  #    > sls_resp_signed - Determines whether the SAML Service Provider will sign `<samlp:logoutResponse>` messages with
  #    > its private key.
  #    > accepted_values:
  #    > - false
  #    > - true
  sls_resp_signed: false
  #    > strict - Setting the SAML Service Provider to strict enables greater protection by performing additional
  #    > checks on the response from the SAML Identity Provider.
  #    > accepted_values:
  #    > - "false"
  #    > - "true"
  strict: 'false'
  #    > want_assertions_encrypted - Determines whether the SAML Service Provider will require assertions sent by the SAML Identity
  #    > Provider to be encrypted with the Service Provider's certificate.
  #    > accepted_values:
  #    > - false
  #    > - true
  want_assertions_encrypted: false
  #    > want_assertions_signed - Determines whether the SAML Service Provider will require assertions sent by the SAML Identity
  #    > Provider to be signed with the Identity Provider's private key.
  #    > accepted_values:
  #    > - false
  #    > - true
  want_assertions_signed: false
  #    > want_attribute_statement - Specifies that the SAML Service Provider expects and requires an attribute statement on the
  #    > `<samlp:Response>` message.
  #    > accepted_values:
  #    > - false
  #    > - true
  want_attribute_statement: false
  #    > want_messages_signed - Indicates that the SAML Service Provider expects `<samlp:Response>`, `<samlp:LogoutRequest>` and
  #    > `<samlp:LogoutResponse>` messages to be signed by the Identity Provider with its private key.
  #    > accepted_values:
  #    > - false
  #    > - true
  want_messages_signed: false
  #    > want_name_id_encrypted - Indicates that the SAML Service Provider expects the NameID sent by the Identity Provider to be
  #    > encrypted with the Service Provider's certificate.
  #    > accepted_values:
  #    > - false
  #    > - true
  want_name_id_encrypted: false
  #    > want_name_id - Specifies that the SAML Service Provider requires a `NameID` element on the `<samlp:Response>`
  #    > message.
  #    > accepted_values:
  #    > - false
  #    > - true
  want_name_id: false
schedule_manager:
  db:
    #    > admin_user - Admin user for the schedule manager database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    # admin_user: <<¬¬ .Values.vault.db.admin_user ¬¬>>
    #    > host - Hostname for the schedule manager database
    # host: <<¬¬ .Values.vault.db.host ¬¬>>
    #    > migrator_user - schedule-manager DB migrator user used for running db migrations
    #    > Note: Hyphen characters as part of this value are supported, but recommended to be avoided.
    #    > If you are using client certificates, set this value to be equal to the common name of the
    #    > \"schedule-manager_migrator\" principal, which can be found in the DB principals artefact.
    #    > regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    # migrator_user: schedule_manager_migrator
    #    > name - Name of the schedule manager database
    #    > Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #    > regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    # name: schedule_manager
    #    > port - Database port to connect to.
    #    > regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    # port: <<¬¬ .Values.vault.db.port | quote ¬¬>>
    #    > user - Username to access the schedule manager database
    #    > Note: Hyphen characters as part of this value are supported, but recommended to be avoided.
    #    > If you are using client certificates, set this value to be equal to the common name of the
    #    > \"calendar\" principal, which can be found in the DB principals artefact.
    #    > regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    # user: schedule_manager
scheduler:
  db:
    #    > admin_user - Admin user for the database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the scheduler database
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Scheduler DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'scheduler_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'scheduler_migrator'
    # Name of the scheduler database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'scheduler'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'scheduler'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the scheduler database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'scheduler'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'scheduler'
secrets_management:
  hashicorp_vault:
    #    > address - The Hashicorp Vault endpoint
    #    > regex: https?:\/\/
    address: 'http://vault.hashicorp-ns.svc.cluster.local:8200'
    #    > auth_k8s_backend - Path where the Kubernetes auth backend for the corresponding Kubernetes cluster is mounted in HashiCorp Vault.
    auth_k8s_backend: kubernetes
    #    > ca_pem - Optional field containing the HashiCorp Vault CA certificate chain as a string. If this field is set, it is used for TLS verification of the HashiCorp Vault server.
    #    > This is useful when the HashiCorp Vault instance is set up with a self-signed certificate.
    # ca_pem: |
    #   -----BEGIN CERTIFICATE-----
    #   intermediate CA
    #   -----END CERTIFICATE-----
    #   -----BEGIN CERTIFICATE-----
    #   root CA
    #   -----END CERTIFICATE-----
    db:
      #    > ca - Optional field describing the location of the Certificate Authority to use for signing database certificates
      #    > accepted_values:
      #    > - hault-kv
      #    > - hault-pki
      # ca: hault-kv
      #    > pki_mount_point - Optional field specifying the mount point to use for the HashiCorp Vault PKI engine for database certificates.
      #    > Defaults to “pki”. Only applies when secrets_management.hashicorp_vault.db.ca: “hault-pki”.
      #    > regex: ^[^\/].*[^\/]$
      # pki_mount_point: pki
    kafka:
      #    > ca - Optional field describing the location of the Certificate Authority to use for signing kafka certificates
      #    > accepted_values:
      #    > - hault-kv
      #    > - hault-pki
      # ca: hault-kv
      #    > pki_mount_point - Optional field specifying the mount point to use for the HashiCorp Vault PKI engine for kafka certificate.
      #    > Defaults to “pki”. Only applies when secrets_management.hashicorp_vault.kafka.ca: “hault-pki”.
      #    > regex: ^[^\/].*[^\/]$
      # pki_mount_point: pki
    #    > namespace - Optional field specifying the HashiCorp Vault namespace, this is an Enterprise feature, should not be set for OSS Vault
    namespace: 'hashicorp-ns'
    #    > secret_mount_point - Path where the KV backend for secrets is mounted.
    #    > regex: ^[^\/].*[^\/]$
    secret_mount_point: 'secret'
    #    > secret_prefix - Path that all secrets for this Vault instance are prefixed with
    secret_prefix: 'myprefix'
    #    > secret_version - Version of the KV backend used for secrets
    #    > accepted_values:
    #    > - "1"
    #    > - "2"
    secret_version: "1"
    #    > vault_installer_hault_role - The name of a HashiCorp Vault role bound to the service account used by the Vault installer ops pod.
    #    > The Vault installer can be used to automatically manage secrets and policies within HashiCorp Vault. To allow this, the policy supplied with the release artifacts should be granted to the role specified.
    #    > This will grant the permissions required by the role used by the installer to manage secrets and policies.
    # vault_installer_hault_role: vault-installer
switchboard:
  db:
    # Hostname for the switchboard database
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Switchboard DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'switchboard_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'switchboard_migrator'
    # Name of the switchboard database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'switchboard'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'switchboard'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the switchboard database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'switchboard'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'switchboard'
tracing:
  exporter:
    # The endpoint for exporters to send traces via Jaeger protobuf protocol to.
    #
    #   Default: ''
    jaeger-endpoint: 'vault-collector.jaeger:14250'
  sampler:
    # Sampler strategy dependent argument. For parentbased_traceidratio, the argument is the sample rate.
    # https://github.com/open-telemetry/opentelemetry-specification/blob/main/specification/sdk-environment-variables.md#general-sdk-configuration
    #
    #   Default: '0'
    arg: '0.01'
    # OpenTelemetry sampler based on https://github.com/open-telemetry/opentelemetry-specification/blob/main/specification/trace/sdk.md#built-in-samplers
    #
    #   Default: 'parentbased_traceidratio'
    #
    #   Accepted values: 'always_off', 'parentbased_traceidratio'
    type: 'parentbased_traceidratio'
vault:
  db:
    # Admin user for the vault database (used for schema initialisation)
    #
    #   Default: the value of vault.db.root_user
    #
    #   Value must satisfy regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the vault database
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Vault DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'vault_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'vault_migrator'
    # Name of the vault database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'vault'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'vault'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Name of the replication slot used in prior releases for streaming to vault services.  Required
    # for deletion of the current slot.
    #
    # Must conform with the regex "^[a-z_][a-z0-9_]{0-59}+$". Usage of lower case alphabets,
    # integers and underscores are valid.
    #
    #   Value must satisfy regex: ^[a-z_][a-z0-9\_]{0,59}$
    replication_slot: 'vault_replication'
    # User used in prior releases for streaming replication from this database to vault services.
    # Required for deletion of the user.
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    replication_user: 'vault'
    # Username for the vault database.
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'vault'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'vault'
webhook_operator:
  # The namespace to deploy the webhook operator to
  #
  #   Default: 'webhook-operator'
  namespace: 'tm-deploy-ns'
workflow_engine:
  # Option to enable support for legacy schemas (below 3.0.0) in workflows engine.  Starlark-based and Python-based Workflow transforms are supported, and this optional configuration setting disables legacy Workflows schema (i.e. all Workflow schema versions prior to 3.0).  There is an identical value for `workflow_simulator`.  This configuration option is temporary and will be removed in Vault 4.0, which will only support Starlark-based transformations. Starlark-based Workflows offer enhanced security posture due to their more restricted runtime and sandbox support.
  #
  # WARNING: When `legacy_schema_versions_enabled` is set to `false`, any Workflows based on schema versions prior to 3.0 will no longer function. The default is `true`, which supports both legacy (Python-based) and current (Starlark-based) Workflow transforms.
  #
  #   Default: 'true'
  #
  #   Accepted values: 'false', 'true'
  legacy_schema_versions_enabled: 'true'
workflow_simulator:
  # The endpoint used for ingress to the workflow simulator
  endpoint: 'workflow-simulator.${env}.icsapi.com'
  # Option to enable support for legacy schemas (below 3.0.0) in workflows simulator.  Starlark-based and Python-based Workflow transforms are supported, and this optional configuration setting disables legacy Workflows schema (i.e. all Workflow schema versions prior to 3.0).  There is an identical value for `workflow_engine`.  This configuration option is temporary and will be removed in Vault 4.0, which will only support Starlark-based transformations. Starlark-based Workflows offer enhanced security posture due to their more restricted runtime and sandbox support.
  #
  # WARNING: When `legacy_schema_versions_enabled` is set to `false`, any Workflows based on schema versions prior to 3.0 will no longer function. The default is `true`, which supports both legacy (Python-based) and current (Starlark-based) Workflow transforms.
  #
  #   Default: 'true'
  #
  #   Accepted values: 'false', 'true'
  legacy_schema_versions_enabled: 'true'
workflows_api:

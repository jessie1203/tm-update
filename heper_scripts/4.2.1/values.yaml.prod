---
# ***********************************************************
# Thought Machine Example Values File:
#  Information:
#   - Values with defaults are set to their default value and commented out.
#   - Docstrings are denoted by '#    >' to differentiate them from default values.
#   - Values without defaults are set to an example value and left uncommented.
#   - Values can depend on one another using Go templating (with Sprig) of the form `svc_url: '<<¬¬.Values.my.instance.url¬¬>>/svc'`.
#     We use the custom delimiters `<<¬¬` and `¬¬>>` to prevent clashes with go templates in the values.
#     Templates will be applied after defaults are merged.
# ***********************************************************

access_control:
  db:
    #    > admin_user - Admin user for the database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the access-control database
    #
    #   Default: the value of vault.db.host
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Access Control DB migrator user used for running DB migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'access_control_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'access_control_migrator'
    # Name of the access-control database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'access_control'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'access_control'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username for the access-control database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'access_control'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'access_control'
access_control_api:
  # The endpoint used for ingress to the access control API
  endpoint: 'access-control-api-gateway.${env}.icsapi.com'
account:
  # Default denomination to be used for an account. This is the default value if no other
  # denomination is given during account creation.
  default_denomination: 'IDR'
  # The permitted denominations for an account. These are the default values if no other
  # denominations list is provided during account creation.
  # Acceptable values are:
  # ["ZMW","CHE","TZS","DOP","DZD","SOS","XUA","SDG","MKD","NIO","UYU","MXN","RUB","UGX","RSD","COP","CDF","BHD","AWG","MOP","SAR","KES","BBD","XPD","BYR",
  # "PEN","EUR","GNF","DKK","BOV","CVE","MVR","KYD","MWK","BTC","NPR","KHR","SGD","XDR","BWP","SRD","CAD","JPY","USS","ALL","MZN","UYI","KZT","MMK","UZS",
  # "ERN","TOP","TTD","BMD","XFU","ETB","SZL","NGN","HRK","XAF","BND","KRW","SLL","PAB","NZD","HTG","GHS","XAG","OMR","SHP","BDT","GIP","IDR","SCR","RWF",
  # "THB","PHP","AFN","LYD","BIF","LKR","XCD","XBD","SBD","STD","XBB","GMD","PYG","RON","MGA","SSP","AMD","PGK","MXV","XSU","XAU","HKD","CHF","CLP","QAR",
  # "AED","XOF","MUR","TWD","MDL","HNL","KPW","TND","BOB","BAM","BTN","TRY","LRD","SEK","IRR","XBC","JOD","BRL","GTQ","TJS","XTS","FKP","PLN","BGN","AZN",
  # "PKR","VEF","TMT","GYD","YER","VUV","CHW","XPF","BZD","INR","ISK","WST","LSL","FJD","XPT","EGP","CUC","XBA","ILS","MAD","USD","UAH","ZAR","CNY","JMD",
  # "ARS","GBP","NOK","MNT","USN","CRC","AUD","DJF","MRO","SYP","LAK","KWD","KGS","IQD","MYR","CZK","COU","AOA","ANG","NAD","HUF","CLF","KMF","LBP","GEL",
  # "VND","CUP"]
  permitted_denominations: '["IDR"]'
attribute_library:
  db:
    #    > admin_user - Admin user for the database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the Attribute Library database
    #
    #   Default: the value of vault.db.host
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Attribute Library DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'attribute_library_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'attribute_library_migrator'
    # Name of the Attribute Library database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'attribute_library'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'attribute_library'
    # Port number to connect to Attribute Library database.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the Attribute Library database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'attribute_library'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'attribute_library'
audit:
  cleanup:
    # Length of time to retain audit log entries
    #
    #   Value must satisfy regex: ^[0-9]+\s(MINUTES|HOURS|DAYS|WEEKS|MONTHS|YEARS)$
    ttl: '1 DAYS'
  db:
    #    > admin_user - Admin user for the database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the audit database. If this is not set, the host will be set to `vault.db.host`.
    #
    #   Default: the value of vault.db.host
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Audit DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'audit_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'audit_migrator'
    # The name of the audit database
    #
    #   Default: 'audit'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'audit'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the audit database
    #
    #   Default: 'audit'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'audit'
  # The endpoint used for ingress to the audit API
  endpoint: 'audit-api-${env}.internal.icsapi.com'
  #    > vault_object_type_policies - The policies that must be satisfied for users to be able to view Audit Logs associated with the specified Vault
  #    > object types. If multiple policies are listed beneath a `vault_object_type`, then at least one must be satisfied by
  #    > a user for them to view Audit Logs associated with the Vault object type.
  #    > This value should be given as a string of valid YAML.
  #    > accepted_type: string
  vault_object_type_policies: |
    default:
      - "default_audit_view"
auth:
  # The size of the cache used for storing validated service account tokens. The cryptographic
  # operations used during authentication require a lot of memory and CPU resources to validate
  # service account tokens. The results of cryptographic operations can be cached to provide
  # additional performance. This value corresponds to the number of valid service account tokens
  # stored in the cache.
  #
  #   Default: '200'
  #
  #   Value must satisfy regex: ^[1-9]\d+$|^\d$
  service_account_cache_size: '200'
balances:
  db:
    # Admin user for the balances database (used for schema initialisation)
    #
    #   Default: the value of vault.db.admin_user
    #
    #   Value must satisfy regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the balances database
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for balances.
    #
    #   Default: the value of vault.db.host
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Balances DB migrator user used for running db migrations
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for balances.
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: the value of vault.db.migrator_user
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'balances_migrator'
    # Name of the Balances database
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for balances.
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: the value of vault.db.name
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'balances'
    # Database port to connect to.
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for balances.
    #
    #   Default: the value of vault.db.port
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the balances database
    # Warning: For Vault versions instances created <2.6 the value must be matching the values of vault db. This can be achieved by not specifying the value to fall back on the default. For new instances created >=2.6 the value can be different if using a separate physical db for balances.
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: the value of vault.db.user
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'balances'
bottomline:
  # The 6-digit code of the instructing bank, used by Bottomline for outbound FPS payments
  instructing_bank_code: '123456'
  # The name of the instructing bank, used by Bottomline for outbound FPS payments
  instructing_bank_name: 'Nikel'
calendar:
  db:
    #    > admin_user - Admin user for the database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the calendar database
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Calendar DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'calendar_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'calendar_migrator'
    # Name of the calendar database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'calendar'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'calendar'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the calendar database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'calendar'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'calendar'
calendar_schedules:
  db:
    #    > admin_user - Admin user for the database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the calendar schedules database
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Calendar Schedules DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'calendar_schedules_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'calendar_schedules_migrator'
    # Name of the calendar schedules database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'calendar_schedules'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'calendar_schedules'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the calendar schedules database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'calendar_schedules'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'calendar_schedules'
clusterstat:
  #    > run_labels - Comma-separated list of labels to run clusterstat tests with.
  #    > Valid values are: hault, monitoring, namespaced_monitoring or all.
  run_labels: all
cockroachdb:
  ca_rotation:
    #    > schedule - Schedule for running the cockroachdb CA rotation job in cron format.
    #    > Ignored if cockroachdb-ca-rotation-pkg not deployed.
    #    > WARNING: Make sure not to schedule for the same time as certificate rotation in the cockroachdb-cert-rotation-pkg, this may lead to unexpected behaviour.
    # schedule: 0 0 15 1 *
  cert_rotation:
    #    > schedule - Schedule for running the cockroachdb certificate rotation job in cron format.
    #    > Ignored if cockroachdb-cert-rotation-pkg not deployed.
    # schedule: 0 0 1 * *
common:
  bank:
    # The identifier for this bank. Must not start with a capital letter or have spaces between
    # characters.
    #
    #   Value must satisfy regex: ^[a-z][A-Za-z0-9\-]+$
    name: 'nikel'
    # Sort code for the bank
    #
    #   Value must satisfy regex: ^[0-9]{2}-[0-9]{2}-[0-9]{2}$
    sort_code: '10-00-00'
  basic_auth:
    # To enable or disable basic authentication. If enabled; user, password and password salted hash values are expected
    #
    #   Default: 'enable'
    #
    #   Accepted values: 'enable', 'disable'
    authentication: 'disable'
    # Password for basic auth credentials applied to ingress.
    #
    # Used to restrict access to Vault instances in dev/test environments.
    #password: 'topsecret'
    # Hash for the above password - should be generated using a command similar to the following:
    #   openssl passwd -apr1 -salt vault topsecret
    #password_hash: '$apr1$vault$r8zSo5z8xGRTLnCCcbS83.'
    # Username for basic auth credentials applied to ingress.
    #
    # Used to restrict access to Vault instances in dev/test environments.
    #username: 'someuser'
  db:
    # Use SSL when connecting to the database.
    #
    #   Default: 'true'
    #
    #   Accepted values: 'true', 'false'
    ssl: 'true'
    # The postgres SSL mode to use when connecting to the database, for all services.
    # For more details, see the Postgres documentation:
    # https://www.postgresql.org/docs/current/libpq-ssl.html
    # Note that 'allow' and 'prefer' are not supported.
    #
    #   Default: 'verify-full'
    #
    #   Accepted values: 'disable', 'require', 'verify-ca', 'verify-full'
    ssl_mode: 'require'

    # Reformat the db username as 'user@host' when establishing a connection. Azure Psql requires this.
    #
    #   Default: 'false'
    #
    #   Accepted values: 'true', 'false'
    use_username_hostname_format: 'false'
	is_non_production: 'true'
  # DEPRECATED - appears unused.
  #
  #   Accepted values: 'true', 'false'
  production_reconciliation: 'false'
  services:
    # The exclusive domain or subdomain name which contains all subdomains used for Vault endpoints
    # protected by SAML in an instance.
    # The protocol and port number must be omitted. For example, if the following websites are n
    # to be protected by SAML: ops.example.com, customer.example.com then the value must be
    # example.com. For multiple instances on the same domain a unique subdomain must be provided.
    # For example, for ops.dev.example.com and ops.staging.example.com the dev instance must be
    # set to dev.example.com and the staging instance must be staging.example.com
    #
    #   Value must satisfy regex: ^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\-]*[a-zA-Z0-9])\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\-]*[A-Za-z0-9])$
    domain: 'icsapi.com'
common_stream_api:
  # The format of public topic messages consumed or produced by the Streaming API
  #
  #   Accepted values: 'json', 'protobuf'
  message_format: 'json'
configuration_layer:
  db:
    #    > admin_user - Admin user for the database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the configuration layer database
    #
    #   Default: the value of vault.db.host
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Configuration Layer DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'configuration_layer_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'configuration_layer_migrator'
    # Name of the configuration layer database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'configuration_layer'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'configuration_layer'
    # Database port to connect to.
    #
    #   Default: '5432'
    #
    #   Value must satisfy regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the configuration layer database - used by services to access db
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'configuration_layer'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'configuration_layer'
contract_db_data_exporter:
  # Controls how frequently a check is made for any new data to be published to
  # `vault.api.v1.products.product_version.created` and
  # `vault.api.v1.products.product_version.parameter.updated` topics. The clock for the next check
  # starts running after all new data has been published. The value can be any "duration string" as
  # is understood by https://golang.org/pkg/time/#ParseDuration.
  #
  #   Default: '3h'
  publish_interval: '1h30m'
contract_executor:
  # Determines how many replicas are deployed as a minimum. This is to ensure Vault can maintain the
  # SLOs for online journeys. For production environments with high volumes this should be set to
  # "medium", for lower volumes "small" should suffice and if no SLOs need to be honoured "demo".
  #
  #   Default: 'medium'
  #
  #   Accepted values: 'medium', 'small', 'demo'
  size: 'demo'
core:
  # The endpoint used for ingress to the core API
  endpoint: 'core-api.${env}.icsapi.com'
data_loader:
  db:
    #    > admin_user - Admin user for the data loader database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    #    > host - Hostname for the data loader database
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Data Loader DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'data_loader_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'data_loader_migrator'
    # Name of the data loader database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'data_loader'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    name: 'data_loader'
    #    > port - Database port to connect to.
    #    > regex: ^([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$
    port: '5432'
    # Username to access the data loader database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'data_loader'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    user: 'data_loader'
data_loader_api:
  # The endpoint used for ingress to the data loader API
  endpoint: 'data-loader-api.${env}.icsapi.com'
  # The format of public topic messages consumed or produced by the data loader stream API
  #
  #   Default: 'protobuf'
  #
  #   Accepted values: 'json', 'protobuf'
  message_format: 'json'
documentation:
  # The endpoint used for ingress to the documentation for vault APIs
  endpoint: 'dd.${env}.icsapi.com'
documents:
  # The endpoint used for ingress to the documents service
  endpoint: 'documents.${env}.icsapi.com'
  # Port that the documents service should listen on
  port: '443'
eplatform:
  db:
    #    > admin_user - Admin user for the database (used for schema initialisation)
    #    > regex: ^[a-zA-Z][a-z0-9A-Z\_]{0,15}$
    admin_user: 'vaultbootstrap'
    # Hostname for the enterprise platform database
    host: 'tm-vault.${env}.db.internal.icsapi.com'
    # Eplatform DB migrator user used for running db migrations
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'eplatform_migrator'
    #
    #   Value must satisfy regex: ^[a-zA-Z\_][a-z0-9A-Z\_-]{0,59}$
    migrator_user: 'eplatform_migrator'
    # Name of the eplatform database
    #
    # Note: Hyphen characters as part of this value are supported, but recommended to be avoided
    #
    #   Default: 'eplatform'
    #
